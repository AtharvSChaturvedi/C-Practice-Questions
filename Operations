#include<stdio.h>
int main() {
	char op;
	int a,b,c;
	printf("Enter two numbers and an operator : ");
	scanf("%d %d %c", &a, &b, &op);
	if(op=='*')
	c=a*b;
		else if(op=='/')
		c=a/b;
			else if(op=='+')
			c=a+b;
				else if(op=='-')
				c=a-b;
					else if(op=='%')
					c=a%b;
						else
						printf("INVALID CHARACTER");
	printf("%d %c %d = %d\n", a, op, b, c);					
}
